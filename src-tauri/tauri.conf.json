{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "plan2go_desktop",
  "version": "0.1.2",
  "identifier": "nl.michielvanboven.plan2go",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Plan2Go",
        "width": 800
      }
    ],
    "withGlobalTauri": true
  },
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "frontendDist": "../public"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {
    "updater": {
      "endpoints": ["https://github.com/TheDutchProgrammers/Plan2Go/releases/latest/download/latest.json"],
      "pubkey": "",
      "windows": {
        "installMode": "passive"
      }
    }
  }
}